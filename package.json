{
  "name": "minimalreactstarter",
  "version": "2.0.0",
  "description": "A minimal React/Express web app starter project",
  "browser": "server/index.js",
  "repository": "github:lighthurst/minimalReactStarter",
  "keywords": [
    "react",
    "starter-template",
    "zero-configuration",
    "build-tools"
  ],
  "author": "Aaron Tesfai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lighthurst/minimalReactStarter/issues"
  },
  "homepage": "https://github.com/lighthurst/minimalReactStarter#readme",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "build": "webpack -d",
    "serve": "node server/index.js",
    "start": "npm run build && npm run serve",
    "build-dev": "webpack -d --watch",
    "serve-dev": "nodemon server/index.js",
    "start-dev": "npm run build-dev & npm run serve-dev",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint"
  },
  "jest": {
    "setupFiles": [
      "./client/spec/setupTestFiles.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  }
}
